<lynx>
	<application>
		<force-quit b="false" />
		<terminal b="false" />
        <threads i="4" />
	</application>
	<netstat>
		<keep-alive i="10" />
		<enable-no-delay b="true" />
	</netstat>
	<listen s="*">
		<hostname s="127.0.0.1" />
		<port i="8080" />
	</listen>
	<site s="*">
		<resource-root s="./tmp/" />
		<host s="*" />
		<route s="/">
			<compression>
				<quality-level i="8" />
				<memory-level i="8" />
				<min-length i="8192" />
				<enabled b="true" />
			</compression>
			<document-root s="./" />
			<allow-directory-listing b="true" />
		</route>
	</site>
</lynx>
